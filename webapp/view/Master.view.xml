<mvc:View controllerName="one.labs.mem_profiler.controller.Master" displayBlock="true" xmlns:mvc="sap.ui.core.mvc"
	xmlns:commons="sap.suite.ui.commons"
	xmlns:core="sap.ui.core"
	xmlns:l="sap.ui.layout" xmlns:viz="sap.viz.ui5.controls" xmlns="sap.m" xmlns:micro="sap.suite.ui.microchart">
	<Page id="MasterPage" title="In-Memory Profiler" showNavButton="true" navButtonPress="onNavBack">
		<content>
			<IconTabBar id="idIconTabBarMulti" expanded="{device>/isNoPhone}" class="sapUiResponsiveContentPadding">
				<items>
					<IconTabFilter icon="sap-icon://area-chart" text="Overview">
						<mvc:XMLView viewName="one.labs.mem_profiler.view.MemoryOverview"/>
					</IconTabFilter>
					<IconTabFilter icon="sap-icon://it-system" text="Platform">
						<mvc:XMLView viewName="one.labs.mem_profiler.view.Platform"/>
					</IconTabFilter>
					<IconTabFilter icon="sap-icon://dimension" text="Business">
						<mvc:XMLView viewName="one.labs.mem_profiler.view.FuncArea"/>
					</IconTabFilter>
				</items>
			</IconTabBar>
		</content>
		<footer></footer>
	</Page>
</mvc:View>